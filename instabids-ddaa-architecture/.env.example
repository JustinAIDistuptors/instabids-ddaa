# InstaBids Domain-Driven Agent Architecture 
# Environment Configuration Example

# Database connection (for Supabase implementation)
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_KEY=your_supabase_anon_key_here
SUPABASE_SECRET=your_supabase_service_role_key_here

# Event bus configuration (for Redis implementation)
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=

# Logging
LOG_LEVEL=info
ENABLE_DETAILED_EVENTS=true

# Pattern enforcement
STRICT_PATTERN_ENFORCEMENT=true

# Domain-specific settings
MAX_BID_AMOUNT=1000000
MIN_BID_AMOUNT=50
